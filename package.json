{
  "name": "E-Parse",
  "version": "1.0.0",
  "description": "G-Parse in an Electron app.",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/ankitpati/eparse.git"
  },
  "keywords": [
    "gparse",
    "PSL",
    "public",
    "suffix",
    "list"
  ],
  "author": "Ankit Pati",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://gitlab.com/ankitpati/eparse/issues"
  },
  "homepage": "https://gitlab.com/ankitpati/eparse#readme",
  "build": {
    "appId": "in.ankitpati.${name}",
    "productName": "${name}",
    "copyright": "Copyright Â© 2019 ${author}",
    "linux": {
      "executableName": "${name}",
      "desktop": {
        "Name": "${name}",
        "Exec": "${name}",
        "Type": "Application",
        "Categories": "Electron;Utility"
      }
    },
    "mac": {
      "category": "public.app-category.utilities"
    }
  },
  "devDependencies": {
    "electron": "^7.1.7",
    "electron-builder": "^22.3.2",
    "eslint": "^6.8.0"
  }
}
